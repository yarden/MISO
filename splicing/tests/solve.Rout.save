> 
> library(splicing)
> 
> set.seed(42)
> options(width=60)
> 
> gene <- createGene(list(c(1,100), c(201,300), c(401,500)),
+                    list(c(1,2), c(1,3), c(1,2,3)))
> 
> reads <- simulateReads(gene, expression=c(2/10, 3/10, 5/10),
+                        noReads=100L, readLength=20)
> 
> mres <- solveIsoOne(gene, reads=reads, readLength=20L)
> mres$expression
[1] 0.1667444 0.2782573 0.5549983
> 
> reads2 <- simulateReads(gene, expression=c(2/10, 3/10, 5/10),
+                         noReads=500L, readLength=20, normalMean=50,
+                         normalVar=50, numDevs=4)
> 
> mres2 <- solveIsoOne(gene, reads=reads2, readLength=20L,
+                      normalMean=50, normalVar=50, numDevs=4)
> mres2$expression
[1] 0.3381114 0.3277335 0.3341551
